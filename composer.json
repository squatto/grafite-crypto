{
    "name": "squatto/grafite-crypto",
    "description": "Cryptography tools for Laravel and Lumen",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Carpenter",
            "email": "scott@payforstay.com",
            "role": "Developer"
        },
        {
            "name": "Matt Lantz",
            "email": "mattlantz@gmail.com",
            "role": "Original package developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^8.0",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "mikey179/vfsstream": "^1.6",
        "orchestra/testbench": "^6.0",
        "laravel/legacy-factories": "^1.0.4"
    },
    "autoload": {
        "psr-4": {
            "Grafite\\Crypto\\": "src/"
        },
        "files": [
          "src/Helpers/crypto.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grafite\\Crypto\\CryptoProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
